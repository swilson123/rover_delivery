{
    "name": "angles",
    "title": "Angles.js",
    "version": "0.5.1",
    "description": "The RAW function collection for working with angles",
    "homepage": "https://github.com/rawify/Angles.js",
    "bugs": "https://github.com/rawify/Angles.js/issues",
    "keywords": [
        "degree",
        "degrees",
        "convert",
        "lerp",
        "radians",
        "direction",
        "geo",
        "compass",
        "cardinal",
        "slope",
        "angle",
        "angles",
        "angular",
        "average",
        "mean",
        "between"
    ],
    "private": false,
    "main": "./dist/angles.js",
    "module": "./dist/angles.mjs",
    "browser": "./dist/angles.min.js",
    "unpkg": "./dist/angles.min.js",
    "types": "./angles.d.ts",
    "exports": {
        ".": {
            "import": {
                "types": "./angles.d.mts",
                "default": "./dist/angles.mjs"
            },
            "require": {
                "types": "./angles.d.ts",
                "default": "./dist/angles.js"
            },
            "browser": {
                "types": "./angles.d.ts",
                "default": "./dist/angles.min.js"
            },
            "default": {
                "types": "./angles.d.ts",
                "default": "./dist/angles.js"
            }
        },
        "./package.json": "./package.json"
    },
    "typesVersions": {
        "<4.7": {
            "*": [
                "angles.d.ts"
            ]
        }
    },
    "sideEffects": false,
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/rawify/Angles.js.git"
    },
    "funding": {
        "type": "github",
        "url": "https://github.com/sponsors/rawify"
    },
    "author": {
        "name": "Robert Eisele",
        "email": "robert@raw.org",
        "url": "https://raw.org/"
    },
    "license": "MIT",
    "engines": {
        "node": "*"
    },
    "directories": {
        "test": "tests"
    },
    "scripts": {
        "build": "crude-build Angles",
        "test": "mocha tests/*.js"
    },
    "devDependencies": {
        "crude-build": "^0.1.2",
        "mocha": "*"
    }
}