    "npm-run-all2": "8.0.4",
    "open": "10.2.0",
    "oxlint": "1.15.0",
    "pony-cause": "2.1.11",
    "postject": "1.0.0-alpha.6",
    "registry-auth-token": "5.1.0",
    "registry-url": "7.2.0",
